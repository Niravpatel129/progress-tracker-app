{
  "expo": {
    "name": "DailyPhotoProgress",
    "slug": "DailyPhotoProgress",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "privacy": "public",
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ],
      [
        "expo-image-picker",
        {
          "photoPermission": "Allow $(PRODUCT_NAME) to access your photos."
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourdomain.dailyphotoprogress",
      "buildNumber": "1.0.4"
    },
    "android": {
      "package": "com.yourdomain.dailyphotoprogress",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["android.permission.CAMERA", "android.permission.RECORD_AUDIO"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "isDevClient": false,
      "eas": {
        "projectId": "e6d2f5f8-0a73-43bc-80a7-4091b1171d93"
      }
    }
  }
}
